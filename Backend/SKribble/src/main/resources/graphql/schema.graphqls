type Query{
	findStoriesByTitle(storyTitleInput: StoryTitleInput!) : [StoryOutput]
}

type Mutation{
	newStory(storyTitleInput: StoryTitleInput!) : String
	newChapter(addChapterForm: AddOrEditChapterForm!) : String
	editChapter(editChapterForm: AddOrEditChapterForm!) : String
	newCharacter(addCharacterForm: AddOrEditCharacterForm!) : String
}