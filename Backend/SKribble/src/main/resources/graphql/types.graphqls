type Chapter{
    chapterNumber: Int
    chapterName: String
    text: String
    mentionedCharacters: [StoryCharacter]
}

type StoryCharacter{
    characterId: String
    name: String
    description: String
}

type StoryOutput{
    id: String
    title: String
    type: ProjectTypes
    chapters: [Chapter]
    characters: [StoryCharacter]
    owner: String
}